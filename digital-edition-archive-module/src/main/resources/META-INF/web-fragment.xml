<web-fragment xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-fragment_3_0.xsd" version="3.0">
  <name>digital-edition-archive_module</name>

  <servlet id="OAI2Provider">
    <servlet-name>OAI2</servlet-name>
    <servlet-class>org.mycore.oai.MCROAIDataProvider</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>OAI2</servlet-name>
    <url-pattern>/oai2</url-pattern>
  </servlet-mapping>

</web-fragment>